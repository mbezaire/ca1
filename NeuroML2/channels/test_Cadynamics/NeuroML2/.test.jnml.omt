# Script for running automated tests on OSB using Travis-CI, see https://github.com/OpenSourceBrain/osb-model-validation

target: LEMS_test_Ca.xml
engine: jNeuroML
mep: .test.mep
experiments:
  v:
    observables:
      spike times:
        file:
          path: lems_ca.dat
          columns: [0,1]
          scaling: [1000, 1000]
        spike detection:
          method: threshold
          threshold: -50
        tolerance: 5e-05

      
